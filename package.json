{
  "name": "reform-gatsby-starter",
  "private": true,
  "description": "Reform Collective Gatsby Starter",
  "version": "0.1.0",
  "author": "Reform Collective",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "15.18.0",
    "@contentful/rich-text-types": "16.3.0",
    "@react-three/drei": "9.88.3",
    "@react-three/fiber": "8.14.5",
    "ahooks": "3.7.8",
    "dotenv": "16.3.1",
    "gatsby": "5.12.7",
    "gatsby-plugin-contentful-optional-fields": "2.2.2",
    "gatsby-plugin-image": "3.12.1",
    "gatsby-plugin-manifest": "5.12.1",
    "gatsby-plugin-netlify": "5.1.1",
    "gatsby-plugin-pnpm-gatsby-5": "1.2.11",
    "gatsby-plugin-sharp": "5.12.1",
    "gatsby-plugin-sitemap": "6.12.1",
    "gatsby-plugin-styled-components": "6.12.0",
    "gatsby-plugin-svgr": "3.0.0-beta.0",
    "gatsby-plugin-tsconfig-paths": "1.0.6",
    "gatsby-source-contentful": "8.12.2",
    "gatsby-transformer-sharp": "5.12.1",
    "gsap": "npm:@gsap/shockingly@3.12.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "6.1.0",
    "stylis": "4.3.0",
    "the-new-css-reset": "1.11.0",
    "three": "0.157.0"
  },
  "devDependencies": {
    "@types/three": "0.157.0",
    "@types/node": "20.8.6",
    "@types/reach__router": "1.3.12",
    "@types/react": "18.2.28",
    "@typescript-eslint/eslint-plugin": "6.8.0",
    "eslint": "8.51.0",
    "eslint-config-reform-collective": "github:reformcollective/eslint-config-reform",
    "stylelint": "15.11.0",
    "stylelint-config-reform-collective": "github:reformcollective/stylelint-config-reform",
    "typescript": "5.2.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "format": "npm run lint:css && eslint . --fix",
    "lint": "eslint .",
    "lint:css": "npx stylelint './src/**/*.{js,jsx,ts,tsx}' --fix || exit 0",
    "start": "gatsby develop -H 0.0.0.0",
    "dev": "npm run start",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc --noEmit",
    "checks": "(npm run type-check || (echo 'Type check failed.' && exit 1)) && (npx eslint . || (echo 'Lint check failed.' && exit 1)) && (npm run build || (echo 'Build failed.' && exit 1)) && echo 'All checks passed.'",
    "preinstall": "npx only-allow pnpm",
    "lighthouse": "bunx lighthouse http://localhost:9000 --view --chrome-flags='--headless'"
  },
  "eslintConfig": {
    "extends": "reform-collective"
  },
  "stylelint": {
    "extends": "stylelint-config-reform-collective"
  },
  "browserslist": [
    "last 99 chrome versions and last 1 year",
    "last 99 ChromeAndroid versions and last 1 year",
    "last 99 Safari versions and last 1 year",
    "last 99 iOS versions and last 1 year",
    "last 99 firefox versions and last 1 year",
    "last 99 FirefoxAndroid versions and last 1 year"
  ]
}
